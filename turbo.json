{
  "$schema": "https://turbo.build/schema.json",
  "tasks": {
    "build": {
      "dependsOn": ["^build"],
      "inputs": ["$TURBO_DEFAULT$"],
      "env": ["NODE_ENV"],
      "outputs": ["dist/**"]
    },
    "test": {
      "inputs": ["$TURBO_DEFAULT$"],
      "env": ["NODE_ENV"],
      "cache": true
    }
  }
}
